#!/bin/bash

#$ -l nc=6
#$ -p -50
#$ -q node.q
export OPENBLAS_NUM_THREADS=6

LATEST=(`ls -t ~/.julia/packages/OnlinePCA/ | head -1`)
gd="~/.julia/packages/OnlinePCA/"$LATEST"/bin/gd"

/usr/bin/time -v julia $gd \
--scale log \
--input AAAAA \
--outdir BBBBB \
--numepoch CCCCC \
--rowmeanlist DDDDD \
--dim EEEEE \
--stepsize FFFFF \
--logdir BBBBB/log >& GGGGG

touch BBBBB/Eigen_vectors.csv
touch BBBBB/Eigen_values.csv
touch GGGGG