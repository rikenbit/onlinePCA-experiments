#!/bin/bash

#$ -l nc=6
#$ -p -50
#$ -q node.q
export OPENBLAS_NUM_THREADS=6

LATEST=(`ls -t ~/.julia/packages/OnlinePCA/ | head -1`)
algorithm971="~/.julia/packages/OnlinePCA/"$LATEST"/bin/algorithm971"

/usr/bin/time -v julia $algorithm971 \
--scale log \
--input AAAAA \
--outdir BBBBB \
--rowmeanlist DDDDD \
--dim EEEEE \
--niter 3 \
--logdir BBBBB/log >& GGGGG

touch BBBBB/Eigen_vectors.csv
touch BBBBB/Eigen_values.csv
touch GGGGG